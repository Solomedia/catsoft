{
	"presets": [
		"next/babel",
		"@zeit/next-typescript/babel",
		"@emotion/babel-preset-css-prop"
	],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"components": "./components",
					"layouts": "./layouts",
					"lib": "./lib",
					"static": "./static",
					"utils": "./utils"
				}
			}
		],
		["emotion", { "inline": true }]
	]
}
