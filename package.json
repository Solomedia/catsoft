{
	"name": "catsoft",
	"scripts": {
		"dev": "nodemon server/index.ts",
		"build": "next build && tsc --project tsconfig.server.json",
		"start": "npm run build && NODE_ENV=production node .next/production-server/index.js",
		"storybook": "start-storybook -p 9001",
		"lint:ts": "tslint --project .",
		"lint:css": "stylelint '**/*.ts'",
		"lint": "npm run lint:css && npm run lint:ts",
		"format": "prettier --write **/*.{js,ts,tsx}"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format && npm run lint"
		}
	},
	"description": "Ecommerce website connected to magento 2.",
	"version": "1.0.0",
	"main": "next.config.js",
	"dependencies": {
		"@emotion/core": "^10.0.5",
		"@emotion/styled": "^10.0.5",
		"@rebass/grid": "^6.0.0-7",
		"@zeit/next-typescript": "^1.1.1",
		"apollo-boost": "^0.1.23",
		"apollo-link-context": "^1.0.12",
		"apollo-link-http": "^1.5.9",
		"emotion": "^10.0.5",
		"emotion-server": "^10.0.5",
		"emotion-theming": "^10.0.5",
		"express": "^4.16.4",
		"graphql": "^14.0.2",
		"graphql-tag": "^2.10.0",
		"isomorphic-fetch": "^2.2.1",
		"isomorphic-unfetch": "^3.0.0",
		"next": "^7.0.2",
		"react": "^16.6.3",
		"react-apollo": "^2.3.3",
		"react-dom": "^16.6.3",
		"typescript": "^3.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@storybook/addon-links": "^4.1.2",
		"@storybook/react": "^4.1.2",
		"@types/express": "^4.16.0",
		"@types/next": "^7.0.5",
		"@types/react": "^16.7.17",
		"@types/rebass__grid": "^6.0.2",
		"@types/storybook__react": "^4.0.0",
		"babel-loader": "^8.0.4",
		"babel-plugin-emotion": "^10.0.5",
		"babel-runtime": "^6.26.0",
		"husky": "^1.3.0",
		"nodemon": "^1.18.9",
		"prettier": "^1.15.3",
		"stylelint": "^9.9.0",
		"stylelint-config-recommended": "^2.1.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.5.1",
		"ts-node": "^7.0.1",
		"tslint": "^5.12.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-react": "^3.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Solomedia/catsoft.git"
	},
	"author": "Solo media dev team",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Solomedia/catsoft/issues"
	},
	"homepage": "https://github.com/Solomedia/catsoft#readme",
